pipeline {
    agent none
    stages {
        stage("Check maven version"){
            steps {
            sleep 15
                echo 'Check Maven version'
                sh 'exit 1'
            }
        }
     post{
      failure
      biuld job: 'declarative-pipeline-post-B-marta', parameters: [
      string(name: 'Maven version', value: "2.0")]
     }
    }
   }
